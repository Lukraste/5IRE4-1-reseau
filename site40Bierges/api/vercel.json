{
    "version": 2,
  
    "builds": [{
      "src": "./index.js",
      "use": "@vercel/node"
    }],
  
    "routes": [{
      "src" : "/(.*)",
      "dest" : "/",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin" : "",
        "Access-Control-Allow-Headers" : "https://react-app-two-topaz.vercel.app",
        "Access-Control-Allow-Credentials" : "true"
      }
    }]
  }
